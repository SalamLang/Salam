تابع سلام {
    نمایش "سلام"

    نمایش فرد(۱۰۰)

    نمایش زوج(۱۰۰)

    اگر فرد(۱۰۰) {
        نمایش "عدد صد فرد است"
    } واگرنه {
        نمایش "عدد صد زوج است"
    }

    اگر فرد(۳.۱۴) {
        نمایش "عدد پی فرد است"
    } واگرنه {
        نمایش "عدد پی فرد نیست"
    }

    اگر فرد("رشته تست") {
        نمایش "رشته فرد است"
    } واگرنه {
        نمایش "رشته فرد نیست"
    }
}
